#别名
jdbc-0.proxool.alias=HOLATEST	
#驱动
jdbc-0.proxool.driver-class=oracle.jdbc.OracleDriver
#连接URL
jdbc-0.proxool.driver-url=jdbc:oracle:thin:@10.130.1.9:1521:holaweb
#用户名
jdbc-0.user=msg
#密码
jdbc-0.password=holamsg
#最大连接数 默认值：15个
jdbc-0.proxool.maximum-connection-count=1000
#最小连接数 默认值：5个
jdbc-0.proxool.minimum-connection-count=3
#最少保持的空闲连接数
jdbc-0.proxool.prototype-count=4
#自动侦察各个连接状态的时间间隔(毫秒),侦察到空闲的连接就马上回收,超时的销毁
jdbc-0.proxool.house-keeping-sleep-time=30000
jdbc-0.proxool.test-before-use=true
jdbc-0.proxool.test-after-use=true
#用于保持连接的测试语句
jdbc-0.proxool.house-keeping-test-sql=select sysdate from DUAL
jdbc-0.proxool.verbose=true
jdbc-0.proxool.statistics=10s,1m,1d
jdbc-0.proxool.statistics-log-level=ERROR